<fop version="1.0">
	<renderers>
		<renderer mime="application/pdf">
			<fonts>
				<font kerning="yes" embed-url="fonts/NanumGothic.ttf" sub-font="NanumGothic">
					<font-triplet name="NanumGothic" style="normal" weight="normal"/>
				</font>
				<font kerning="yes" embed-url="fonts/NanumGothicBold.ttf" sub-font="NanumGothicBold">
					<font-triplet name="NanumGothicBold" style="normal" weight="bold"/>
				</font>
			</fonts>
		</renderer>
	</renderers>
</fop>
