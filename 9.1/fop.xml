<fop version="1.0">
	<renderers>
		<renderer mime="application/pdf">
			<fonts>
				<font kerning="yes" embed-url="fonts/malgun.ttf" sub-font="Malgun Gothic">
					<font-triplet name="Malgun Gothic" style="normal" weight="normal"/>
				</font>
				<font kerning="yes" embed-url="fonts/malgunbd.ttf" sub-font="Malgun Gothic Bold">
					<font-triplet name="Malgun Gothic Bold" style="normal" weight="bold"/>
				</font>
			</fonts>
		</renderer>
	</renderers>
</fop>
